    "stand": 1.8,
    "move": 2.75,
    "sneak": 1.15,
    "lie": 0.7,
    "aim": 0.15
  },
  "move_speed": {
    "base": 0.9,
    // 瞄准情况
    "aim": 0.84
  },
  // 开放的配件槽。未指定的槽位默认关闭。全部配件槽类型有:
  // scope, stock, muzzle, grip, laser, extended_mag
  "allow_attachment_types": [
    "scope",
    "stock",
    "muzzle",
    "extended_mag"
  ]
